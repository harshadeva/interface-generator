{
    "name": "harshadeva/interface-generator",
    "description": "Simplify Laravel development by generating repository and interface files with automatic service provider binding",
    "keywords": ["interface","maker","repository","binder","service provider"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Harshadeva",
            "email": "hpbandara94@gmail.com"
        }
    ],
    "require":{
        "php":"^8.3.0"
    },
    "autoload": {
        "psr-4": {
            "Harshadeva\\InterfaceGenerator\\": "src/"
        }
    },
    "extra": {
       "laravel":{
            "providers":["Harshadeva\\InterfaceGenerator\\InterfaceGeneratorProvider"]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {}
}
